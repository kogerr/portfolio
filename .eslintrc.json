{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-undef": 1,
        "no-unused-vars": 1,
        "eqeqeq": "warn",
        "camelcase": "error",
        "eol-last": ["error", "always"],
        "no-tabs": "error",
        "no-multiple-empty-lines": "error",
        "no-extra-parens": "warn",
        "new-cap": ["error", { "capIsNewExceptions": ["express.Router"] }]
    },
    "env": {
        "amd": true,
        "node": true,
        "es6": true
    }
}